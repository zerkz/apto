{
  "name": "apto",
  "description": "Plugin that provides replacement APIs for configuring elements that have no REST/SOAP API equvalient on the Salesforce platform.",
  "version": "0.1.0",
  "author": "Zack Whipkey Github:@zerkz npm:zdware",
  "scope": "@zdware",
  "bugs": "https://github.com/zerkz/apto/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^3",
    "@salesforce/core": "^2",
    "@types/cheerio": "^0.22.29",
    "@types/node": "^16.0.0",
    "cheerio": "^1.0.0-rc.10",
    "puppeteer": "^10.1.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^2",
    "@salesforce/dev-scripts": "^0",
    "@salesforce/prettier-config": "^0",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/jsforce": "^1.9.29",
    "@types/mocha": "^8",
    "@types/puppeteer": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-oclif": "^3.1",
    "eslint-config-prettier": "^8",
    "eslint-config-salesforce": "^0",
    "eslint-config-salesforce-typescript": "^0",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsdoc": "^35",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "globby": "^11",
    "husky": "^4",
    "mocha": "^8",
    "nyc": "^15",
    "prettier": "^2",
    "pretty-quick": "^3",
    "rimraf": "^3.0.2",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zerkz/apto",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "apto": {
        "description": "Replacement APIs for configuration that only can be done via Salesforce UI/browser."
      },
      "apto:cache": {
        "description": "Modify the Platform cache."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "zerkz/apto",
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
